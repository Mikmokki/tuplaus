# Database configuration for PostgreSQL (running in container called "db-1")
POSTGRES_USER=username
POSTGRES_PASSWORD=password
POSTGRES_DB=database

# Database configuration for Flyway (used for database migrations)
FLYWAY_USER=username
FLYWAY_PASSWORD=password
FLYWAY_URL=jdbc:postgresql://db-1:5432/database

# Database configuration for Deno's PostgreSQL driver
PGUSER=username
PGPASSWORD=password
PGHOST=db-1
PGPORT=5432
PGDATABASE=database

DATABASE_URL=postgresql://db-1:5432/database    